{
    "manifest_version": 2,

    "name": "Dota 2 Lounge Helper",
    "description": "Helper extension for Dota 2 Lounge.",
    "version": "0.4",

    "content_scripts": [
        {
            "matches": ["http://*.dota2lounge.com/*"],
            "js": ["util.js"]
        },
        {
            "matches": ["http://*.dota2lounge.com/myprofile"],
            "js": ["async.js", "jquery-2.1.1.js", "jquery.tablesorter.min.js", "bet-stats.js"],
            "css": ["bet-stats.css"]
        },
        {
            "matches": ["http://*.dota2lounge.com/*"],
            "js": ["jquery-2.1.1.js", "item-value.js"]
        }
    ],

    "permissions": [
        "storage",
        "http://*.dota2lounge.com/*",
        "http://steamcommunity.com/market/*"
    ]
}
