{
    "manifest_version": 2,

    "name": "Dota 2 Lounge Helper",
    "description": "Helper extension for Dota 2 Lounge.",
    "version": "0.2",

    "content_scripts": [
        {
            "matches": ["http://*.dota2lounge.com/myprofile"],
            "js": ["bet-stats.js"],
            "css": ["bet-stats.css"]
        }
    ],

    "permissions": [
        "http://*.dota2lounge.com/*"
    ]
}
